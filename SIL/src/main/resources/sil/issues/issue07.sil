// Only comparisons between null constants and variable identifiers
method issueNull(param: Ref) {
    var a: Ref
    var b: Ref
    var c: Ref

    a := null
    b := null
    c := new()

    assert(null == null)
    assert(a == null)
    assert(null == b)
    assert(c != null)

    if (param == null) {
        assert(param == null)
    } else {
        assert(param != null)
    }

    //:: ExpectedOutput(sample.assert.failed)
    assert(false)
}

