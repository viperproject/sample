
field f: Ref

method foo(this: Ref, that: Ref)
  //requires acc(this.f, write)
  //requires acc(that.f, write)
{
    var x: Ref
    var i: Int
    i := 0

    this.f := that

    while (i < 10)
        //invariant acc(this.f, 1/2)
        //invariant acc(this.f.f, write)
    {
        x := new()
        this.f.f := x
        i := i + 1
    }
}