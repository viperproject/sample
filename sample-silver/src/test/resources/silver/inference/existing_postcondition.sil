field f: Ref

method foo_pre(a: Ref, b: Ref, c: Ref)
    requires acc(a.f)
    requires acc(b.f)
    requires acc(c.f)
{}

method foo(a: Ref, b: Ref, c: Ref)
    ensures acc(a.f)
    ensures acc(b.f)
{
    b.f := null
    c.f := null
}

method bar_pre(a: Ref, read: Perm)
    requires none < read && read < write
    requires acc(a.f, read) && acc(a.f.f, write)
{}

method bar(a: Ref)
    ensures acc(a.f.f)
{}
