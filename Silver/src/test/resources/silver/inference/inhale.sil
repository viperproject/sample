
field f: Ref

method foo_pre(b: Ref)
    requires true

method foo(b: Ref)
{
    var a: Ref
    a := b
    inhale acc(b.f, write)
    a.f := null
}

method bar_pre(a: Ref, read: Perm)
    requires none < read && read < write
    requires acc(a.f, read)
{}

method bar(a: Ref)
    // requires acc(a.f, read) && read > none
{
    inhale acc(a.f.f)
}
