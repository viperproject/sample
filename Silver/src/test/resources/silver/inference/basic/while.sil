
field f: Int

method t5_pre(n: Int, r: Ref) returns ()
    requires acc(r.f, write)
{}

method t5(n: Int, r: Ref) returns ()
{
    var i: Int := 2

    while (i < r.f)
        //invariant acc(r.f, write)
    {
        i := i + 1
    }
}

method t9_pre(r: Ref) returns ()
    requires acc(r.f, write)
{}

method t9(r: Ref) returns ()
{
    var j: Int := 2
    while (j < r.f)
        //invariant acc(r.f, write)
    {
        j := j + 1
    }
    //assert j >= r.f
}
