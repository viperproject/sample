
method test_assignVariable_zero() {
	var x: Int
	x := 0
	assert(x == 0)
}

method test_assignVariable_notnull() {
    var x: Ref
    var y: Ref
    x := new()
    y := x
    assert(x != null && null != y && x == y)
}

