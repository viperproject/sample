
field f: Int

method t5(n: Int, r: Ref) returns ()
    //requires n > 0
    //requires acc(r.f, write)
    //ensures acc(r.f, write)
{
    var i: Int := 2

    while (i < r.f)
        //invariant acc(r.f, write)
    {
        i := i + 1
    }
}

method t9(r: Ref) returns ()
    //requires acc(r.f, write)
    //ensures acc(r.f, write)
{
    var j: Int := 2
    while (j < r.f)
        //invariant acc(r.f, write)
    {
        j := j + 1
    }
    assert j >= r.f
}